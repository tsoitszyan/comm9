        <figcaption id="ctl00_PlaceHolderMain_ucc_ArticleImage_ctl01_caption2">
            <div id="ctl00_PlaceHolderMain_ucc_ArticleImage_ctl01_rhf_Caption2_label" style='display:none'>Image Caption</div><div id="ctl00_PlaceHolderMain_ucc_ArticleImage_ctl01_rhf_Caption2__ControlWrapper_RichHtmlField" class="ms-rtestate-field" style="display:inline" aria-labelledby="ctl00_PlaceHolderMain_ucc_ArticleImage_ctl01_rhf_Caption2_label"></div>
